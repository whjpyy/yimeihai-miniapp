<view class="case-box" bindtap="jumpLink">
  <view class='item'>
    <view>
      <image class='before' src="{{ infos.beforePlastic }}"></image>
      <view class='absolute_icon before'>
        <image src='/assets/icon_case.png'></image>
        <text>Before</text>
      </view>
    </view>
    <view>
      <image src="{{ infos.afterPlastic }}"></image>
      <view class='absolute_icon after'>
        <image src='/assets/icon_case.png'></image>
        <text>After</text>
      </view>
    </view>
  </view>
  <view class='text'>
    <view class='xm'>
      <view class='title'>整形项目：</view>
      <text>{{infos.position}}</text>
    </view>
    <view class='people'>
      <view class='row'>
        <text>姓名：</text>
        <text class='val'>{{infos.name}}</text>
      </view>
      <view class='row' style='text-align:center'>
        <text>年龄：</text>
        <text class='val'>{{infos.age}}</text>
      </view>
      <view class='row' style='text-align:right'>
        <view class='btn'>美容日记</view>
      </view>
    </view>
  </view>
</view>