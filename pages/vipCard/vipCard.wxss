/* pages/vipCard/vipCard.wxss */
.banner image {
  width: 100%;
  height: 495rpx;
}

.list {
  padding: 10rpx;
}
.list .item {
  border-top: rgba(211, 211, 211, .33) solid 1rpx;
  width: 700rpx;
  margin: 0 auto;
  padding: 45rpx 0; 
}
.list .item:first-child {
  border-top: none;
}
.list .image {
  width:288rpx;
  height:262rpx;
  position: relative;
}
.list .image image {
  width: 100%;
  height: 100%;
}
.list .over-tag {
  background-color: rgba(34, 34, 34, .43);
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
}
.list .over-tag text {
  width:190rpx;
  height:74rpx;
  background:rgba(255,255,255,1);
  border-radius:4rpx;
  font-size:30rpx;
  font-weight:800;
  color:rgba(255,141,134,1);
  text-align: center;
  line-height: 74rpx;
}
.list .infos {
  height:262rpx;
  margin-left: 30rpx;
}
.list .name {
  width: 100%;
  font-size:30rpx;
  font-weight:bold;
  color:rgba(25,25,25,1);
  line-height:40rpx;
  max-height: 80rpx;
  overflow: hidden;
}
.list .support {
  width: 100%;
  font-size:26rpx;
  font-weight:400;
  color:rgba(114, 114, 114, 1);
  line-height:40rpx;
  max-height: 80rpx;
  overflow: hidden;
  margin-top: 10rpx;
}
.list .support text {
  color:rgba(250,159,76,1);
}
.list .box {
  width: 100%;
  margin-top: 10rpx;
}
.list .price {
  font-size:36rpx;
  font-weight:bold;
  color:rgba(230, 133, 77, 1);
}
.list .price .unit {
  font-size:17rpx;
  font-weight:bold;
  color:rgba(230, 133, 77, 1);
  margin-left: 8rpx;
}
.list .disable {
  color:rgba(92,92,92,1);
}
.list .disable .unit {
  color:rgba(137,137,137,1);
}
.list .btn {
  width: 176rpx;
  height: 82rpx;
  position: relative;
  top: 5rpx;
}
.list .btn image {
  width: 100%;
  height: 100%;
}
.btnNow {
  height:107rpx;
  background:rgba(255,135,126,1);
  
  font-size:36rpx;
  font-weight:800;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 107rpx;
}